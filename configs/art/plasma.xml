<?xml version='1.0' encoding='UTF-8'?>
<pyvcp>
  <labelframe text="Home Switches">
  <font>("Helvetica",12)</font>
  <hbox>
    <led>
      <halpin>"home_j0"</halpin>
      <size>"45"</size>
      <on_color>"green"</on_color>
      <off_color>"gray"</off_color>
    </led>
